<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <servers>
        <server>
            <id>geodesy.operations</id>
            <username>${env.AWS_ACCESS_KEY_ID}</username>
            <password>${env.AWS_SECRET_ACCESS_KEY}</password>
        </server>
        <server>
            <id>geodesy.operations.prod</id>
            <username>${env.AWS_ACCESS_KEY_ID_PROD}</username>
            <password>${env.AWS_SECRET_ACCESS_KEY_PROD}</password>
        </server>
    </servers>
</settings>
